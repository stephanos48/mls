@model IEnumerable<mls.Models.Practice>

@{
    ViewBag.Title = "Download";
}

<h2>Download</h2>

<table id="ii" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th></th>
            <th>
                @Html.DisplayNameFor(model => model.PracticeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PicUrl)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.PracticeId })|
                    @Html.ActionLink("Details", "Details", new { id = item.PracticeId })|
                    @Html.ActionLink("Delete", "Delete", new { id = item.PracticeId })|
                    @Html.ActionLink("Download", "Download1", new { id = item.PracticeId })|
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PracticeId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PicUrl)
                </td>
            </tr>
        }
    </tbody>
</table>